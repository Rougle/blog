
{:dev true
 :port 3000
 ;; when :nrepl-port is set the application starts the nREPL server on load
 :nrepl-port 7000

 ; set your dev database connection URL here
 :database-url "postgresql://db/db_dev?user=dev_user&password=mypasswd"

 ;; set jwt-sercret and hashing alg
 :jwt-secret "jwtsecret"
 :jwt-alg :hs512

 ;; set registration secret
 :register-secret "secret"

 }
